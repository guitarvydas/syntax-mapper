Odin0Dpass2 {
  program = item+
  item =
    | packageDef -- package
    | importDef -- import
    | space -- space
    | any -- other

  importDef =
    | "import" spaces string -- unnamed
    | "import" spaces id spaces string -- named

  packageDef = "package" id

  id = idfirst idrest*
  idfirst = letter | "_"
  idrest = alnum | "_"
  string = dq (~dq any)* dq
  dq = "\""
  comment = "//" (~"\n" any)*
  space += comment
}

